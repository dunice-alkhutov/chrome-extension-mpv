{
  "name": "MPV START",
  "description": "twitch/youtube watcher",
  "version": "0.0.1",
  "manifest_version": 2,
  "background": {
    "persistent": false,
    "scripts": [
      "js/bundle.js"
    ]
  },
  "permissions": [
    "contextMenus",
    "nativeMessaging"
  ],
  "icons": {
    "16": "icons/mpv_16.png",
    "48": "icons/mpv_48.png",
    "128": "icons/mpv_128.png"
  }
}